<div class="container-fluid row">
  <div class="col-xl-12 col-md-12 col-sm-12">
    <div class="card">
      <div class="card-body">
        <h5>Buchungen von <%= @user.user_name %> (<%= @user.email %>)</h5>
        <table class="table">
          <thead>
          <tr>
            <th scope="col">Platz-ID</th>
            <th scope="col">Typ</th>
            <th scope="col">Info</th>
            <th scope="col">Reserviert am</th>
          </tr>
          </thead>
          <tbody>
          <% @reservations.each do |res| %>
            <% desk = Desk.find_by(external_id: res.name) %>
            <tr>
              <td><%= res.name %></td>
              <td><%= desk.kind %></td>
              <td><%= desk.notes %></td>
              <td><%= res.date %></td>
              <td>
              </td>
            </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </div>
    <div class="col-xl-12 col-md-12 col-sm-12">
      <%= link_to('ZurÃ¼ck', users_path, class: current_class?(users_path)) %>
    </div>
  </div>
</div>